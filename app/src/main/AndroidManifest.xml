<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="crickettv.preditionscore.cricinfo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Main_Ads.VideoPlayer_App"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/icon128"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/icon128"
        android:supportsRtl="true"
        android:theme="@style/Theme.HdCricketLiveScore"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Main_Ads.VideoPlyer_Splace_Activity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Main_Ads.admob_ads.MainAdsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Main_Ads.ContinueActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Main_Ads.admob_ads.Privacy_PolicyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Main_Ads.SkipActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Main_Ads.thankyou"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Main_Ads.VideoPlayer_AutoQurekaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity1"
            android:screenOrientation="portrait" />
        <activity
            android:name=".NewsDetailView"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true" />

        <activity
            android:name="com.facebook.ads.AudienceNetworkActivity"
            android:hardwareAccelerated="true" />
    </application>

</manifest>